// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // Prisma < 5.10
  // directUrl = env("DATABASE_URL_UNPOOLED")
}

// npx prisma generate
generator client {
  provider = "prisma-client-js"
}

// npx prisma db push
model User {
  id    String @id @default(cuid())
  email String @unique
  name  String
}
